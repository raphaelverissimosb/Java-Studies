package cursojava.classes;

public class Aluno {
	
	String nome;
	int idade;
	String dataNascimento;
	String registroGeral;
	String numeroCpf;
	String nomaMae;
	String nomePai;
	String dataMatricula;
	String nomeEscola;
	String serieMatriculado;
	
	public Aluno() {
		
	}
	public Aluno(String nomePadrao) {
		this.nome = nomePadrao;
	}
}
